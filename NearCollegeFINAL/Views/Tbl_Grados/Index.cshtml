@model IEnumerable<NearCollegeFINAL.Models.Tbl_Grados>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Institución Jorge Eliecer Gaitán Ayala</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreGrado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JornadaGrado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DisponibilidadCupoGrado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreGrado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JornadaGrado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DisponibilidadCupoGrado)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdGrado }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdGrado }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdGrado })
        </td>
    </tr>
}

</table>
